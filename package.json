{
  "name": "@peter.naydenov/visual-controller-for-svelte3",
  "description": "Tool for building a micro-frontends(MFE) based on Svelte component",
  "version": "2.1.2",
  "license": "MIT",
  "author": "Peter Naydenov",
  "main": "dist/visual-controller-for-svelte.umd.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/visual-controller-for-svelte.esm.mjs",
      "require": "./dist/visual-controller-for-svelte.cjs",
      "default": "./dist/visual-controller-for-svelte.umd.js"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*",
    "./src/*": "./src/*"
  },
  "scripts": {
    "dev": "vite",
    "build": "rollup -c",
    "preview": "vite preview",
    "test": "cypress open --component --browser chrome"
  },
  "dependencies": {
    "ask-for-promise": "^3.0.1"
  },
  "devDependencies": {
    "@peter.naydenov/notice": "^2.4.0",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "cypress": "^13.17.0",
    "svelte": "^4.2.19",
    "vite": "^6.2.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PeterNaydenov/visual-controller-for-svelte3"
  },
  "keywords": [
    "'micro",
    "frontend",
    "MFE",
    "svelte3",
    "svelte",
    "widgets",
    "visual",
    "controller'"
  ]
}
